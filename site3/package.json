{
  "name": "express-typescript-app",
  "version": "1.0.0",
  "description": "Simple Express TypeScript application",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsc && node dist/server.js",
    "debug": "tsc && node --inspect dist/server.js",
    "debug:ts": "ts-node --inspect src/server.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.28",
    "ai": "^5.0.106",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}
